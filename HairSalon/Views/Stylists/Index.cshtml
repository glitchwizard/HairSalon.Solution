@{
}

<h1>Stylists</h1>

@if (@Model.Count == 0)
{
    <h3>No stylists have been added yet!</h3>
}

@foreach (var stylist in Model)
{
    <h3><a href='/stylists/@stylist.id'>@stylist.Name</a></h3>
}

<h5><a href="/stylists/new">Click here</a> to add a new stylist.</h5>

<p><a href='/'>Back home</a></p>